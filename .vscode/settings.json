{
  "files.exclude": {
    "amplify/.config": true,
    "amplify/**/*-parameters.json": true,
    "amplify/**/amplify.state": true,
    "amplify/**/transform.conf.json": true,
    "amplify/#current-cloud-backend": true,
    "amplify/backend/amplify-meta.json": true,
    "amplify/backend/awscloudformation": true
  },
  "cSpell.words": [
    "AWSJSON",
    "chakra",
    "codegen",
    "METHODTODOLOGY",
    "partialize",
    "postmvp",
    "reparent",
    "reparenting",
    "TOPBAR",
    "triaging",
    "unfavoriting",
    "unprioritized"
  ],
  "todo-tree.filtering.excludeGlobs": [
    "**/node_modules/**",
    "**/.git/**",
    "**/.vscode/**",
    "**/dist/**",
    "**/build/**",
    "**/out/**",
    "**/amplify/**",
  ],
  "todo-tree.tree.scanMode": "open files",
  "todo-tree.regex.regex": "(TODO\\(P[1-5]\\)|TODO\\(stretch\\)|TODO\\(postmvp\\)|TODO\\?|TODO[ \\t]|FIXME[ \\t]|BUG[ \\t]|HACK[ \\t]|NOTE[ \\t]|REVIEW[ \\t])",
  "todo-tree.general.tagGroups": {},
  "todo-tree.general.tags": [
    "TODO(P1)",
    "TODO(P2)",
    "TODO(P3)",
    "TODO(P4)",
    "TODO(P5)",
    "TODO(stretch)",
    "TODO(postmvp)",
    "TODO?",
    "FIXME",
    "BUG",
    "HACK",
    "NOTE",
    "REVIEW"
  ],
  "todo-tree.highlights.customHighlight": {

    "TODO(P1)": {
        "icon": "flame",
        "type": "line",
        "foreground": "#FF3B30",
        "background": "#2A0B0B"
    },
    "TODO(P2)": {
        "icon": "arrow-up",
        "type": "line",
        "foreground": "#FF9500",
        "background": "#2A1600"
    },
    "TODO(P3)": {
        "icon": "alert",
        "type": "line",
        "foreground": "#FFD60A",
        "background": "#2A2100"
    },
    "TODO(P4)": {
        "icon": "beaker",
        "type": "line",
        "foreground": "#0A84FF",
        "background": "#001826"
    },
    "TODO(P5)": {
        "icon": "rocket",
        "type": "line",
        "foreground": "#BF5AF2",
        "background": "#1C0B2A"
    },
    "TODO(stretch)": {
        "icon": "star",
        "type": "line",
        "foreground": "#30D158",
        "background": "#071F10"
    },
    "TODO(postmvp)": {
        "icon": "arrow-right",
        "type": "line",
        "foreground": "#5AC8FA",
        "background": "#0B1C26"
    },
    "TODO?": {
        "icon": "question",
        "type": "line",
        "foreground": "#8E8E93",
        "background": "#1C1C1E"
    }
  }
}